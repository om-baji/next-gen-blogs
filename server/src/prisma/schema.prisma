generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        String   @id @default(cuid())
  name      String?
  email     String   @unique
  createdAt DateTime @default(now())
  notes     Notes[]
  blogs     Blogs[]
}

model Notes {
  id      String @id @default(cuid())
  title   String
  content String
  userId  String
  email   String
  User    User   @relation(fields: [userId], references: [id])
}

model Blogs {
  id     String @id @default(cuid())
  title  String
  body   Json
  userId String
  User   User   @relation(fields: [userId], references: [id])
}
